{
    "name": "StreetView Screenshotter",
    "version": "0.1",
    "description": "Google StreetView Screenshotter with GPS",
    "background": {
        "scripts": ["piexifjs/piexif.js", "background.js", "shortcuts.js"],
        "persistent": false
    },
    "permissions": ["activeTab"],
    "manifest_version": 2,
    "browser_action": {
        "default_title": "StreetView Screenshot",
        "default_icon": "images/icon.png"
    },
    "commands": {
        "toggle-gmap-ui-block": {
            "suggested_key": {
                "default": "Alt+Shift+C",
                "windows": "Alt+Shift+C"
            },
            "description": "Toggle Google Map UI Block"
        }
    }
}